
-- THIS IS A PROGRAM
SUGOD

    LIHOK a() NUMERO PUNDOK{
        IULI 100
    }
    
    LIHOK lt(NUMERO a, NUMERO b) TINUOD PUNDOK{
        IULI a < b
    }

    LIHOK gte(NUMERO a, NUMERO b) TINUOD PUNDOK{
        IULI a >= b
    }

    LIHOK add(NUMERO a, NUMERO b) NUMERO PUNDOK{
        IULI a + b
    }

    LIHOK multiply(NUMERO a, NUMERO b) NUMERO PUNDOK{
        IULI a * b
    }

    LIHOK divide(NUMERO a, NUMERO b) NUMERO PUNDOK{
        IULI a / b
    }
    
